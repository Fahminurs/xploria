  <section id="game" class="drag-drop">
    <div class="container">
      <h2 class="section-title short-stack-bold" data-translate="gameTitle">Fun Learning with Drag & Drop!</h2>
      <p class="gameSubtitle short-stack-regular" data-translate="gameSubtitle">Match the shapes with their correct shadows!</p>
      <div class="section-divider"></div>
      <div class="game-container">
        <div id="game-area" class="game-area">
          <div id="shapes-container-left" class="shapes-container">
            <div id="shape-circle" class="shape shape-circle" draggable="true" data-shape="circle"
              style="background-color: #FF5733;"></div>
            <div id="shape-square" class="shape shape-square" draggable="true" data-shape="square"
              style="background-color: #33FF57;"></div>
          </div>
          <div class="drop-zones">
            <div id="zone-circle" class="drop-zone" data-shape="circle">
              <div class="shadow-shape">
                <div class="shape-circle" style="background-color: #D1D5DB;"></div>
              </div>
            </div>
            <div id="zone-square" class="drop-zone" data-shape="square">
              <div class="shadow-shape">
                <div class="shape-square" style="background-color: #D1D5DB;"></div>
              </div>
            </div>
            <div id="zone-triangle" class="drop-zone" data-shape="triangle">
              <div class="shadow-shape">
                <div class="shape-triangle" style="border-bottom-color: #D1D5DB;"></div>
              </div>
            </div>
            <div id="zone-star" class="drop-zone" data-shape="star">
              <div class="shadow-shape">
                <svg width="50" height="50" viewBox="0 0 24 24" fill="#D1D5DB">
                  <polygon
                    points="12,2 15.09,8.26 22,9.27 17,14.14 18.18,21.02 12,17.77 5.82,21.02 7,14.14 2,9.27 8.91,8.26" />
                </svg>
              </div>
            </div>
          </div>
          <div id="shapes-container-right" class="shapes-container">
            <div id="shape-triangle" class="shape shape-triangle" draggable="true" data-shape="triangle"
              style="border-bottom-color: #3357FF;"></div>
            <div id="shape-star" class="shape shape-star" draggable="true" data-shape="star">
              <svg width="50" height="50" viewBox="0 0 24 24" fill="#F3FF33">
                <polygon
                  points="12,2 15.09,8.26 22,9.27 17,14.14 18.18,21.02 12,17.77 5.82,21.02 7,14.14 2,9.27 8.91,8.26" />
              </svg>
            </div>
          </div>
        </div>
        <div id="success-message" class="success-message" style="display: none;">
          <div class="success-icon">
            <svg xmlns="http://www.w3.org/2000/svg" width="48" height="48" viewBox="0 0 24 24" fill="none"
              stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <path d="M22 11.08V12a10 10 0 1 1-5.93-9.14" />
              <polyline points="22 4 12 14.01 9 11.01" />
            </svg>
          </div>
          <h3 class="success-title short-stack-bold" data-translate="greatJob">Great job!</h3>
          <p class="success-text short-stack-regular" data-translate="successText">You matched all the shapes correctly!</p>
          <button id="play-again" class="btn btn-primary" data-translate="playAgain">Play Again</button>
        </div>
      </div>
    </div>
  </section>

  <section id="stem" class="stem">
    <div class="container">
      <h2 class="section-title short-stack-bold" data-translate="discoverSTEM">Discover STEM Projects</h2>
      <div class="section-divider"></div>
      <div class="stem-categories">
        <button class="stem-category aicoding active short-stack-regular" data-category="aicoding">
          <span class="hugeicons--ai-scan"></span>
          <span data-translate="aicoding">AI Coding</span>
        </button>
        <button class="stem-category iotdeveloper short-stack-regular" data-category="iotdeveloper">
          <span class="lucide--circuit-board"></span>
          <span data-translate="iotdeveloper">IOT Developer</span>
        </button>
        <button class="stem-category robotics short-stack-regular" data-category="robotics">
          <span class="ix--robotic-arm"></span>
          <span data-translate="robotics">Robotics</span>
        </button>
        <button class="stem-category webbase short-stack-regular" data-category="webbase">
          <span class="mdi--web"></span>
          <span data-translate="webbase">Web Based Programming</span>
        </button>
        <button class="stem-category basic short-stack-regular" data-category="basic">
          <span class="hugeicons--python"></span>
          <span data-translate="basic">Basic Programming</span>
        </button>
      </div>
      <div class="stem-content">
        <!-- Content will be dynamically inserted here by JavaScript -->
      </div>
    </div>
  </section>

